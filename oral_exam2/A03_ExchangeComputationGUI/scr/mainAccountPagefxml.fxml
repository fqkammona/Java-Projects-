                  <Insets bottom="10.0" left="20.0" right="10.0" top="10.0" />
               </VBox.margin>
            </Button>
            <Button alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#deleteAccount" prefHeight="26.0" prefWidth="157.0" text="Delete Acount">
               <VBox.margin>
                  <Insets bottom="10.0" left="20.0" right="10.0" top="10.0" />
               </VBox.margin>
            </Button>
            <Button alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#takeMeToExchangeGUIfxml" prefHeight="26.0" prefWidth="157.0" text="Log Out">
               <VBox.margin>
                  <Insets bottom="10.0" left="20.0" right="10.0" top="10.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
      <Label layoutX="20.0" layoutY="73.0" text="Account Number:" />
      <Label layoutX="20.0" layoutY="149.0" text="Balance:" />
      <TextField fx:id="numberAccountDisplay" editable="false" layoutX="153.0" layoutY="68.0" />
      <TextField fx:id="balanceAmountDisplay" editable="false" layoutX="153.0" layoutY="144.0" />
   </children>
</AnchorPane>
