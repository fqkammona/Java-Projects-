<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="228.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="exchangeGUIController">
   <children>
      <Label layoutX="14.0" layoutY="14.0" text="Welcome ">
         <font>
            <Font size="35.0" />
         </font>
      </Label>
      <VBox layoutX="343.0" layoutY="29.0" prefHeight="226.0" prefWidth="218.0">
         <children>
            <Button alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#accountInformation" prefHeight="26.0" prefWidth="157.0" text="Account Information">
               <VBox.margin>
                  <Insets bottom="10.0" left="20.0" right="10.0" top="10.0" />
               </VBox.margin>
            </Button>
            <Button alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#takeMeToWithdrawPage" prefHeight="26.0" prefWidth="157.0" text="Withdraw">
               <VBox.margin>
                  <Insets bottom="10.0" left="20.0" right="10.0" top="10.0" />
               </VBox.margin>
            </Button>
            <Button alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#deleteAccount" prefHeight="26.0" prefWidth="157.0" text="Delete Acount">
               <VBox.margin>
                  <Insets bottom="10.0" left="20.0" right="10.0" top="10.0" />
               </VBox.margin>
            </Button>
            <Button alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#takeMeToExchangeGUIfxml" prefHeight="26.0" prefWidth="157.0" text="Log Out">
               <VBox.margin>
                  <Insets bottom="10.0" left="20.0" right="10.0" top="10.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
      <Label disable="true" layoutX="14.0" layoutY="86.0" prefHeight="21.0" prefWidth="157.0" text="Account Number">
         <font>
            <Font size="17.0" />
         </font>
      </Label>
      <Label disable="true" layoutX="14.0" layoutY="142.0" prefHeight="21.0" prefWidth="157.0" text="Account Balance">
         <font>
            <Font size="17.0" />
         </font>
      </Label>
      <Label fx:id="accountNumberDisplay" disable="true" layoutX="180.0" layoutY="86.0" prefHeight="21.0" prefWidth="157.0">
         <font>
            <Font size="17.0" />
         </font>
      </Label>
      <Label fx:id="accountBalanceDisplay" disable="true" layoutX="180.0" layoutY="142.0" prefHeight="21.0" prefWidth="157.0">
         <font>
            <Font size="17.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
